{
  "name": "Proactive Bruin",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Bear"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "content_security_policy":"script-src 'self' 'unsafe-inline' 'sha256-bHouoRuLqiMD3EdzM09puwwsFyUMVrW6I/kb+oqCWDs=' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com https://www.gstatic.com; object-src 'self'"
}